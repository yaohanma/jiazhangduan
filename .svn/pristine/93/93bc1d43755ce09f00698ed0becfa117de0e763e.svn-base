	/*added by stt 最新考试成绩——start*/
	function latestExam(){
		ysAjax("/exam/init", {
			page: 1,
			pageSize: 10
		}, function(data) {
			if(data.retCode == 'SUCCESS') {
				if(data.examList != null && data.examList.length > 0) {
						var item = data.examList[0];
						$('#latestExam p').eq(0).html(item.examName);
						$('#latestExam p').eq(2).html(item.startTime+'~'+item.endTime);
						$('#latestExam').parent('div').click(function(){
							window.location.href = '/html/exam_schedule/exam_results.html?examId='+item.examId+'&flag=1';
						});
				}else{
					$('#latestExam').hide();
					$('#latestExam').next('div .cell-right').hide();
					$('.no_data').show();
				}
			}else{
				showAlert('服务器异常，请稍后重试');
			}
		}, function(err) {
			showAlert('服务器异常，请稍后重试');
		});
	}
