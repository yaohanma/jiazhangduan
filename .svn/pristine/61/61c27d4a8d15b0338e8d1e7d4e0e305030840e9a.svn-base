	.aligncenter {
		clear: both;
		display: block;
		margin: auto;
	}
	/*通知样式*/
	.navbar-item{
		background-image: url(/static/images/study_img/icon_remind.png);
	    background-repeat: no-repeat;
	    background-position-y:center;
	    background-position-x:1.2rem;
	    background-size:25%;
	    
	}
	/*有通知 样式*/
	.navbar-item.on{
		background-image: url(/static/images/study_img/icon_remind_msg.png);
	    background-repeat: no-repeat;
	    background-position-y:center;
	    background-position-x:1.2rem;
	    background-size:25%;
	    
	}
	/*input*/
	.div-bor{
		width: 100%;
		height: 0.58rem;
		position: relative;
		border: 1px solid #dcdcdc;
		border-radius: 4px;
	}
	.div-bor i{
		width:0.25rem ;
		height:0.58rem;
		display: inline-block;
		background: url(/static/images/study_img/icon_search@2x.png) center no-repeat;
		background-size: 100%;
		position: absolute;
		left: 0.15rem;
	}
	.div-bor input{
		width: 90%;
		position: absolute;
		right: 0.15rem;
		border: 0;
		font-size: 0.12rem;
		line-height: 0.58rem;
		text-indent: 0.2rem;
		color: #b2b2b2;
	}
	input::-webkit-input-placeholder {
		color: #b2b2b2;
      }
      .input_box{
      	width: 100%;
      	height: 100%;
      	background: rgba(225,225,225,.9);
      	position: fixed;
      	top: 0.9rem;
      	z-index: 999;
      	display: none;
      }
      .input_box .m-cell{
      	margin-bottom: 0;
      }
	/*banner*/
	.banner_box{
		width: 100%;
		height: 2.8rem;
		margin-bottom: 0.2rem;
	}
	.banner_box .slider-pagination > .slider-pagination-item{
		width: 0.17rem;
		height: 0.17rem;
	}
	.banner_box .slider-pagination > .slider-pagination-item.slider-pagination-item-active{
		background: rgb(4,147,240);
	}
	/*文章*/
	.article_box{
		padding:0 0.3rem;
	}
	.article_box div{
		margin-top: 0.15rem;
		font-size: 0.3rem;
		color: #333;
		padding-bottom: 0.4rem;
	}
	.article_box img{
		border-radius: 10px;
		height: 2.8rem;
	}
	/*新增消息推送 样式*/
	.message_box{
		width: 100%;
		margin-bottom: 0.2rem;
		background: #fff;
	}
	.null_box{
		width: 100%;
		height: 1.12rem;
		line-height: 1.12rem;
		color: #888a97;
		font-size: 0.26rem;
		text-align: center;
		
		display: none;
		
	}
	/*.notice_title{
		border-bottom: 1px solid #e5e5e5;
	}*/
	.notice_title span{
		width: 2.1rem;
		height: 0.42rem;
		line-height: 0.42rem;
		border-radius: 4px;
		color: #fff;
		font-size: 0.3rem;
		text-align: center;
		
	}
