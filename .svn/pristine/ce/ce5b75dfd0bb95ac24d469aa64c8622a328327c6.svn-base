	<body style="background: #fff;">
		<section class="g-flexview">
			<header class="m-navbar" style="">
				<a id="backApp" class="navbar-item">
					<i class="back-ico"></i>
				</a>
				<div class="navbar-center">
					<span class="navbar-title"></span>
				</div>
			</header>
			<section class="g-scrollview">
				<div class="logo_box">
					<img src="/static/images/logo_img/login_img.png" />
				</div>
				<div class="logo_form">
					<div class="m-cell">
						<div class="cell-item">
							<input id="telnum" class="cell-input" placeholder="请输入账号/手机号" type="number" pattern="[0-9]*" oninput="if(value.length>11)value=value.slice(0,11)" />
							<i id="telnum_icon" class="icon-error" style="display: none;"></i>
						</div>
						<div class="cell-item">
							<input id="password" type="password" class="cell-input" placeholder="请输入密码" oninput="if(value.length>8)value=value.slice(0,8)" />
							<i id="password_icon" class="icon-error" style="display: none;"></i>
						</div>
						<div class="cell-item checkbo_box">
							<!--<div class="checkbox">
							      <label><input type="checkbox" name=""></label>记住密码
							</div>-->
							<div class="cell-right" style="color: #a0a4b1;">
								<a href="/html/login/fotget_password.html">忘记密码</a>
							</div>
						</div>

					</div>
					<button type="button" class="btn-block login_btn">登录</button>
					<div class="Account_box">
						<a href="/html/login/login_index.html">手机号登录/注册</a>
					</div>
				</div>
			</section>
		</section>
		<script src="/static/plugins/ydui/js/ydui.js"></script>
		<script src="/static/js/yswxcommon.js"></script>
		<script>
		</script>
	</body>

</html>