			} 
		
			//显示学生内容
			function showContent(stu_id,schoolId){
				for(var i=0; i<array.length; i++){
					if(stu_id != array[i]){
                        $("#content"+array[i]).removeClass("tab-active")
					}
				}
                $("#content"+stu_id).addClass("tab-active")
                if(schoolId == "jzdxnxx201905"){
                    //虚拟学校id--> 对应C端孩子
                    $(".b-class").addClass("hide-item");
                    $(".c-class").removeClass("hide-item");
                }else{
                    $(".c-class").addClass("hide-item");
                    $(".b-class").removeClass("hide-item");
                }
			}
		
		</script>
</body>

</html>